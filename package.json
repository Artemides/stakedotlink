{
  "name": "staking-contracts-v2",
  "version": "1.0.0",
  "description": "LinkPool V2 Staking Contracts",
  "license": "MIT",
  "scripts": {
    "start": "npx hardhat node --network hardhat",
    "test": "npx hardhat test --network hardhat",
    "deploy": "ts-node --files scripts/deploy/1-deploy-test-contracts.ts && ts-node --files scripts/deploy/2-deploy-0.1.ts && ts-node --files scripts/deploy/3-deploy-1.0.ts && ts-node --files scripts/deploy/4-deploy-merkle-distributor.ts",
    "deploy-0.1": "ts-node --files scripts/deploy/2-deploy-0.1.ts",
    "deploy-1.0": "ts-node --files scripts/deploy/3-deploy-1.0.ts",
    "deploy-operator-vcs": "ts-node --files scripts/linkStrategies/deploy-operator-vcs.ts",
    "deploy-community-vcs": "ts-node --files scripts/linkStrategies/deploy-community-vcs.ts",
    "mint-initial-sdl": "ts-node --files scripts/other/mint-initial-sdl.ts",
    "luv-deyta": "npx hardhat run scripts/test/luv-deyta.ts",
    "mock-data": "ts-node --files scripts/test/mock-data.ts",
    "start-mock-data": "node scripts/test/start-mock-data.js",
    "compile": "npx hardhat compile",
    "tsc": "tsc",
    "prettier": "prettier --check \"contracts/**/*.sol*\" \"test/**/*.ts*\" \"scripts/**/*.ts*\"",
    "prettier:fix": "prettier --write \"contracts/**/*.sol*\" \"test/**/*.ts*\" \"scripts/**/*.ts*\"",
    "solhint": "solhint contracts/**/*.sol"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@openzeppelin/hardhat-upgrades": "^1.21.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.14",
    "chai": "^4.3.6",
    "commander": "^9.4.1",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.5.4",
    "fs-extra": "^11.1.0",
    "hardhat": "^2.11.2",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.2",
    "@openzeppelin/contracts": "^4.7.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@prb/math": "^2.5.0",
    "solidity-bytes-utils": "^0.8.0"
  }
}
